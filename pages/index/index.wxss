/**index.wxss**/
@import "../../wxParse/wxParse.wxss";
.placeholder{
    margin: 5px;
    padding: 0 10px;
    text-align: center;
    background-color: #EBEBEB;
    height: 2.3em;
    line-height: 2.3em;
    color: #cfcfcf;
}
.weui-cell{
    padding: 12px 15px 12px 35px;
}
.left_border{
  border-left: solid 3px #795548;
}
.search-bar{
  position:fixed;
  z-index:1000;
  width:100%;
}
.search_input{
    border-radius:50rpx;
    margin:10rpx;
    border:1px solid #795548;
    color: #795548;
    text-align:center;
}

/* 弹出层 */
.modal-box{
  position:fixed;
  width:100%;
  height:100%;
  top:0px;
  bottom:20px;
  background:rgba(0,0,0,0.4);
  overflow: hidden;
  }

.modal-body{
  position:relative;
}

.modal-content{
  overflow: hidden;
  border-radius: 100rpx;
  height:500px;
  background-color: white;
}